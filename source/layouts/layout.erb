<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "Alecamp" %></title>  
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
    <link rel="shortcut icon" href="images/favicon.ico" >
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <script src="https://use.typekit.net/wow4nut.js"></script>
    <script>try{Typekit.load({ async: true });}catch(e){}</script>
    <%= stylesheet_link_tag "screen" %>
    <body class="<%= page_classes %> <%= current_page.data.section %>">
        <div id="main">
<!--           <div class="line"></div> -->
<!--             <div class="logo">
                <a href="/"><h1>Ale<em>camp</em></h1></a>
            </div> -->
          <div class="nav">
            <a class="alecamp" id="alecamp" href="/">Alecamp</a>
            <a class="portafolio" id="portafolio" href="/">Portafolio</a>
            <a class="pricing" id="pricing" href="/pricing">Pricing</a>
            <a class="contact" id="contact" href="/contact">Contact</a>
          </div>
          <div class="container">
            <%= yield_content :fullpage %>
            <%= yield %>
          </div>
        </div>
      <%= javascript_include_tag  "application" %>  
    </body>
  </head>
</html>